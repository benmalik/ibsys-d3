    <system:String x:Key="WindowStart-labelINPUT">Input File</system:String>
    <system:String x:Key="WindowStart-labelVERKAUFSPROGNOSE">Sales</system:String>
    <system:String x:Key="WindowStart-labelPUFFER">Warehouse</system:String>
    <system:String x:Key="WindowStart-labelNACHBESTELLUNG">Order Quantities</system:String>
